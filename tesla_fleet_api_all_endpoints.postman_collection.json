      "item": [
        {
          "name": "charging_history",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://developer.tesla.com/api/1/dx/charging/history",
              "protocol": "https",
              "host": ["developer", "tesla", "com"],
              "path": ["api", "1", "dx", "charging", "history"],
              "query": [
                { "key": "endTime", "value": "", "description": "End time of the window to download charging history for (e.g. 2023-07-28T11:43:45-07:00)" },
                { "key": "pageNo", "value": "", "description": "Current page number." },
                { "key": "pageSize", "value": "", "description": "Number of records per page." },
                { "key": "sortBy", "value": "", "description": "Field to sort by." },
                { "key": "sortOrder", "value": "", "description": "Sort order (ASC or DESC)." },
                { "key": "startTime", "value": "", "description": "Start time of the window to download charging history for (e.g. 2023-07-27T11:43:45-07:00)" },
                { "key": "vin", "value": "", "description": "Vehicle Identification Number (VIN) of the selected vehicle." }
              ]
            },
            "description": "Returns the paginated charging history.\n\nParameters:\n- endTime: End time of the window to download charging history for (e.g. 2023-07-28T11:43:45-07:00)\n- pageNo: Current page number.\n- pageSize: Number of records per page.\n- sortBy: Field to sort by.\n- sortOrder: Sort order (ASC or DESC).\n- startTime: Start time of the window to download charging history for (e.g. 2023-07-27T11:43:45-07:00)\n- vin: Vehicle Identification Number (VIN) of the selected vehicle."
          }
        },
        {
          "name": "charging_invoice",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://developer.tesla.com/api/1/dx/charging/invoice/:id",
              "protocol": "https",
              "host": ["developer", "tesla", "com"],
              "path": ["api", "1", "dx", "charging", "invoice", ":id"],
              "variable": [
                { "key": "id", "value": "", "description": "The unique identifier for the charging event." }
              ]
            },
            "description": "Returns a charging invoice PDF for an event from charging history.\n\nParameters:\n- id: The unique identifier for the charging event (path parameter)."
          }
        },
        {
          "name": "charging_sessions (business fleet only)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "https://developer.tesla.com/api/1/dx/charging/sessions",
              "protocol": "https",
              "host": ["developer", "tesla", "com"],
